unset LC_ALL; export LC_ALL
unset LANGUAGE; export LANGUAGE

prefix=@prefix@
exec_prefix=@exec_prefix@
. @sysconfdir@/ha.d/shellfuncs

case "$1" in
'start')
        ;;
'stop')
        ;;
*)
        echo "Usage: $0 { start | stop }"
        ;;
esac
exit 0
